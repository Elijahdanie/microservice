{
    "compilerOptions": {
      "target": "es2017",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "outDir": "./dist",
      "strict": true,                  
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": false,
      "skipLibCheck": true,
      "strictNullChecks": false,
      "pretty": true,
      "noImplicitAny": false,
      "resolveJsonModule": true,
    },
    "exclude": [
      "node_modules",
    ]
  }